<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rjkj.cf.bbibm.kjds.product.supplier.mapper.SupplierProductMapper">

  <resultMap id="supplierProductMap" type="com.rjkj.cf.bbibm.kjds.product.supplier.entity.SupplierProduct">
                  <id property="id" column="id"/>
                        <result property="productId" column="product_id"/>
                        <result property="productSku" column="product_sku"/>
                        <result property="productName" column="product_name"/>
                        <result property="variantColor" column="variant_color"/>
                        <result property="variantSize" column="variant_size"/>
                        <result property="amount" column="amount"/>
                        <result property="status" column="status"/>
                        <result property="supplierId" column="supplier_id"/>
                        <result property="image" column="image"/>
            </resultMap>
</mapper>
